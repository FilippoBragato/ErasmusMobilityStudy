    "import numpy as np\n",
    "from math import factorial as fc\n",
    "\n",
    "def my_fun(d, x, y):\n",
    "    if(not d-x-y<0):\n",
    "        return 1 - ((fc(d-x)*fc(d-y)/fc(d-x-y))/fc(d))\n",
    "    else:\n",
    "        return 1\n",
    "\n",
    "n = 11\n",
    "for x in range(n):\n",
    "    for y in range(n):\n",
    "        print(x,y,my_fun(n-1,x,y))"
   ]
  }
